  },
  "scripts": {
    "start": "vite serve",
    "build": "npx tsc && vite build",
    "preview": "vite preview",
    "lint": "yarn prettier ./src --check && yarn run eslint && yarn run stylelint",
    "lint:fix": "yarn prettier ./src --write && yarn run eslint:fix && yarn run stylelint:fix",
    "eslint": "npx eslint --ext .jsx,.js,.tsx,.ts src/",
    "eslint:fix": "npx eslint --fix --ext .jsx,.js,.tsx,.ts src/",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "stylelint:fix": "stylelint \"src/**/*.scss\" --fix",
    "test": "vitest --run",
    "test-watch": "vitest"
  },
  "dependencies": {
